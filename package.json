{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "ISC",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"src/test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "ENV=DEVELOPMENT nest start --watch",
    "start:debug": "ENV=DEVELOPMENT nest start --debug --watch",
    "start:prod": "ENV=PRODUCTION node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "ENV=TEST jest -i",
    "test:cov": "ENV=TEST jest -i --coverage",
    "test:debug": "ENV=TEST node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "ENV=TEST jest -i --config src/test/jest-e2e.json",
    "generate-migragion": "ts-node ./src/migrations/generate-migration.ts",
    "apply-migration": "typeorm-ts-node-commonjs migration:run -d ./src/migrations/data-source.ts",
    "rollback-migration": "typeorm-ts-node-commonjs migration:revert -d ./src/migrations/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.1",
    "@nestjs/cqrs": "11.0.2",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/swagger": "11.0.7",
    "@nestjs/typeorm": "11.0.0",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cookie-parser": "1.4.7",
    "express-basic-auth": "1.2.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.24",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.0",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "jest": "29.5.0",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "typescript-eslint": "8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/common/(.*)$": "<rootDir>/common/$1",
      "@/settings/(.*)$": "<rootDir>/settings/$1",
      "@/films/(.*)$": "<rootDir>/modules/cinema/film/$1",
      "@/cartoons/(.*)$": "<rootDir>/modules/cinema/cartoon/$1",
      "@/serials/(.*)$": "<rootDir>/modules/cinema/serial/$1",
      "@/movies/(.*)$": "<rootDir>/modules/cinema/movies/$1",
      "@/data-migrations/(.*)$": "<rootDir>/migrations/data-migration/$1",
      "@/admin/(.*)$": "<rootDir>/modules/admin/$1",
      "@/bcrypt-module/(.*)$": "<rootDir>/modules/bcrypt/$1",
      "@/admin-auth/(.*)$": "<rootDir>/modules/auth/admin/$1",
      "@/sessions/(.*)$": "<rootDir>/modules/auth/session/$1",
      "@/jwt-module/(.*)$": "<rootDir>/modules/jwt/$1"
    }
  }
}
